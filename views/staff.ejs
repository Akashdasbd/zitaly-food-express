<section id="container" class="sub-page">
	<div class="wrap-container zerogrid">
		<div class="crumbs">
			<ul>
				<li><a href="index.html">Home</a></li>
				<li><a href="staff.html">Staff</a></li>
			</ul>
		</div>
		<div id="main-content">
			<div class="wrap-content">
				<div class="chef">
					<div class="row">
                        <%  staffs.forEach((staff)=>{%>
						<div class="col-1-4">
							<div class="wrap-col">
								<div class="zoom-container">
									<a href="#">
										<img src="<%= staff.photo %>" />
									</a>
								</div>
								<h3><%= staff.name %></h3>
								<ul class="social t-center">
									<li><a href="<%= staff.social.tw %>"><i class="fa fa-twitter"></i></a></li>
									<li><a href="<%= staff.social.fb %>"><i class="fa fa-facebook"></i></a></li>
									<li><a href="<%= staff.social.li %>"><i class="fa fa-linkedin"></i></a></li>
									<li><a href="<%= staff.social.ig %>"><i class="fa fa-instagram"></i></a></li>
								</ul>
							</div>
						</div>
                        <% }) %>
					</div>
				</div>
			</div>
		</div> 
	</div>
</section>
