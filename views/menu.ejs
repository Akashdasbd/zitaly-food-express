<section id="container" class="sub-page">
	<div class="wrap-container zerogrid">
		<div class="crumbs">
			<ul>
				<li><a href="index.html">Home</a></li>
				<li><a href="menu.html">Menu</a></li>
			</ul>
		</div>
		<div id="main-content">
			<div class="wrap-content">
				<div class="row">
					<% menuItems.forEach((item) => { %>
					<div class="col-1-3">
						<div class="wrap-col">
							<h3><%= item.category %></h3>
							<% item.items.forEach((food)=>{ %>
							<div class="post">
								<a href="#"><img src="<%= food.img %>"/></a>
								<div class="wrapper">
								  <h5><a href="#"><%= food.title %></a></h5>
								  <span><%= food.price %></span>
								</div>
							</div>
							<% }) %>

						</div>
					</div>
					<% }) %>

				</div>

			</div>
		</div> 
	</div>
</section>